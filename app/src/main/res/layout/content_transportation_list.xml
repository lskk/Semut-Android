<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_transportation_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="5dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:paddingTop="5dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="project.bsts.semut.TransportationListActivity"
    tools:showIn="@layout/activity_transportation_list">
    <ListView
        android:id="@+id/list_transportation"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="#01ffffff"
        android:scrollbars="none" >
    </ListView>
</RelativeLayout>
